        "1_page_payment_0_tv2": "\u0906\u092a\u0923 \u092d\u0935\u094d\u092f \u0916\u0930\u0947\u0926\u0940 \u0915\u0930\u0923\u0947 \u0928\u093f\u0935\u0921\u0932\u094d\u092f\u093e\u0938, \u0906\u092a\u0932\u094d\u092f\u093e Google Play \u0916\u093e\u0924\u094d\u092f\u093e\u0935\u0930 \u0926\u0947\u092f \u0936\u0941\u0932\u094d\u0915 \u0906\u0915\u093e\u0930\u0932\u0947 \u091c\u093e\u0908\u0932 \u0906\u0923\u093f \u0935\u0930\u094d\u0924\u092e\u093e\u0928 \u0915\u093e\u0932\u093e\u0935\u0927\u0940\u091a\u094d\u092f\u093e \u0938\u092e\u093e\u092a\u094d\u0924\u0940\u091a\u094d\u092f\u093e \u0905\u0917\u094b\u0926\u0930 24 \u0924\u093e\u0938\u093e\u0902\u091a\u094d\u092f\u093e \u0906\u0924 \u0928\u0942\u0924\u0928\u0940\u0915\u0930\u0923\u093e\u0938\u093e\u0920\u0940 \u0906\u092a\u0932\u094d\u092f\u093e \u0916\u093e\u0924\u094d\u092f\u093e\u0935\u0930 \u0936\u0941\u0932\u094d\u0915 \u0906\u0915\u093e\u0930\u0932\u0947 \u091c\u093e\u0908\u0932."
    },
    "4_page_payment": {
        "4_page_payment_0_tv1": "\u0935\u093e\u0908\u091f \u092c\u093e\u0924\u092e\u0940 \u0906\u092a\u0932\u094d\u092f\u093e \u0906\u0930\u094b\u0917\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u0906\u0923\u093f \u092e\u0928\u093e\u0938\u093e\u0920\u0940 \u0935\u093e\u0908\u091f \u0905\u0938\u0924\u093e\u0924",
        "4_page_payment_0_tv2": "\u0906\u092a\u0924\u094d\u0924\u0940, \u092d\u094d\u0930\u0937\u094d\u091f\u093e\u091a\u093e\u0930 \u0906\u0923\u093f \u0905\u0915\u094d\u0937\u092e\u0924\u0947\u0928\u0947 \u0935\u0930\u094d\u0924\u092e\u093e\u0928\u092a\u0924\u094d\u0930\u0947 \u0906\u0923\u093f \u091f\u0940\u0935\u094d\u0939\u0940 \u092a\u094d\u0930\u0938\u093e\u0930\u0923\u0947 \u0915\u093e \u092d\u0930\u0932\u0940 \u091c\u093e\u0924\u093e\u0924? \u0939\u0947 \u0905\u0938\u0942 \u0936\u0915\u0924\u0947 \u0915\u093e\u0930\u0923 \u0906\u092a\u0923 \u0915\u0933\u093e\u092f\u0932\u093e \u0928 \u0932\u093e\u0917\u0924\u093e \u0928\u093f\u0930\u093e\u0936\u093e\u091c\u0928\u0915 \u0915\u0925\u093e\u0902\u0915\u0921\u0947 \u0906\u0915\u0930\u094d\u0937\u093f\u0924 \u091d\u093e\u0932\u094b \u0906\u0939\u094b\u0924, \u0905\u0938\u0947 \u092e\u093e\u0928\u0938\u0936\u093e\u0938\u094d\u0924\u094d\u0930\u091c\u094d\u091e \u091f\u0949\u092e \u0938\u094d\u091f\u093e\u092b\u0930\u094d\u0921 \u092e\u094d\u0939\u0923\u0924\u093e\u0924.",
        "4_page_payment_1_tv1": "\u0916\u0942\u092a \u0935\u093e\u0908\u091f \u092c\u093e\u0924\u092e\u0940 \u0906\u092a\u0932\u094d\u092f\u093e\u0932\u093e \u0906\u091c\u093e\u0930\u0940 \u092c\u0928\u0935\u0942 \u0936\u0915\u0924\u0947",
        "4_page_payment_1_tv2": "\u092a\u094d\u0930\u0924\u094d\u092f\u0947\u0915 \u092c\u093e\u0924\u092e\u0940 \u091a\u0947\u0924\u093e\u0935\u0923\u0940 \u0915\u093f\u0902\u0935\u093e \u092c\u094d\u0930\u0947\u0915\u093f\u0902\u0917 \u0938\u094d\u091f\u094b\u0930\u0940\u0938\u0939 \u0906\u092a\u0932\u0947 \u091c\u0917 \u092a\u0941\u0922\u0947 \u0906\u0923\u093f \u092a\u0941\u0922\u0947 \u0938\u0902\u0915\u091f\u093e\u092e\u0927\u094d\u092f\u0947 \u0922\u0915\u0932\u0932\u0947\u0932\u0947 \u0926\u093f\u0938\u0924\u0947. \u0939\u0947 \u0906\u092a\u0932\u094d\u092f\u093e \u0935\u093e\u0924\u093e\u0935\u0930\u0923\u093e\u0935\u0930 \u092a\u0930\u0902\u0924\u0941 \u0906\u092a\u0932\u094d\u092f\u093e \u0906\u0930\u094b\u0917\u094d\u092f\u093e\u0935\u0930\u0939\u0940 \u0917\u0902\u092d\u0940\u0930 \u0938\u0902\u0915\u091f \u0906\u0923\u0924 \u0906\u0939\u0947.",
